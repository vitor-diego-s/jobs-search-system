# Profile extracted from resume (auto-generated by: python main.py extract-profile)
# Review and edit before running: python main.py generate-config

# Your full name
name: "Jane Doe"

# Job search queries (3-8 recommended)
# Each becomes a separate LinkedIn search
search_keywords:
  - "Senior Python Engineer"
  - "Staff Backend Engineer"
  - "Senior Software Engineer Python"

# Your seniority level: junior | mid | senior | staff | principal | director
# Controls experience_level filter in generated settings
seniority: "senior"

# Technical skills and technologies from your resume
# Used for scoring (title match bonus) â€” NOT as hard filters
scoring_keywords:
  - "Python"
  - "FastAPI"
  - "Django"
  - "PostgreSQL"
  - "Kubernetes"
  - "AWS"
  - "Docker"
  - "REST"
  - "microservices"

# Keywords to exclude from search results
exclude_keywords:
  - "Junior"
  - "PHP"
  - "Ruby"
  - "Frontend"
  - "React Native"
  - ".NET"

# Total years of professional experience (optional)
years_of_experience: 8

# Workplace preferences: remote | hybrid | onsite
preferred_workplace:
  - "remote"

# LinkedIn geo IDs (optional, empty = worldwide)
# 92000000 = Worldwide, 103644278 = US, 101165590 = UK
preferred_geo_ids: []
